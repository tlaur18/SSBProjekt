<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ssb.presentation_layer.controllers.vum_document_controllers.SagsåbningController">
    <children>
        <TabPane fx:id="tabPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab text="Side 1">
                    <content>
                        <ScrollPane fitToWidth="true">
                            <content>
                                <fx:include source="sagsåbning_content1.fxml" />
                            </content>
                        </ScrollPane>
                    </content>
                </Tab>
                <Tab text="Side 2">
                    <content>
                        <ScrollPane fitToWidth="true">
                            <content>
                                <fx:include source="sagsåbning_content_2.fxml" />
                            </content>
                        </ScrollPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox alignment="CENTER_RIGHT" spacing="16.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveBtnHandler" text="Gem" />
                <Button mnemonicParsing="false" onAction="#cancelBtnHandler" text="Annuller" />
            </children>
            <VBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
